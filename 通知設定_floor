function sendLineNotify_floor(message) {
  var accessToken = "fuOl7IFrdADXH6g1fZ0dB6xsDZjSoOn8EmH0ZoxjvL6"; // 発行したアクセストークンを設定
  
  var options = {
    "method": "POST",
    "headers": {
      "Authorization": "Bearer " + accessToken,
    },
    "payload": {
      "message": message,
    },
  };
  
  UrlFetchApp.fetch("https://notify-api.line.me/api/notify", options);
}
